<view
    wx:for="{{buttons}}"
    wx:for-item="button"
    wx:key="{{button.lang}}"
    catchtouchstart="streamRecord"
    catchtouchend="endStreamRecord"
    data-conf="{{button}}"
    class="button-press"
>
    <image
        class="button-background"
        src="./voice.png"
        width="64rpx"
        width="64rpx"
    ></image>
</view>
